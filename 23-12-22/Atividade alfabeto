  funcao escreveLetra(cadeia letra){
    varreMatriz(letra)
  }

  funcao varreMatriz(cadeia letra){

    inteiro matriz[7][5]

    se(letra=="A"){
      matriz = matrizA
    }senao se(letra=="B"){
      matriz = matrizB
    }senao se(letra=="C"){
      matriz = matrizC
    }senao se(letra=="D"){
      matriz = matrizD
    }senao se(letra=="E"){
      matriz = matrizE
    }senao se(letra=="F"){
      matriz = matrizF
    }senao se(letra=="G"){
      matriz = matrizG
    }senao se(letra=="H"){
      matriz = matrizH
    }senao se(letra=="I"){
      matriz = matrizI
    }senao se(letra=="J"){
      matriz = matrizJ
    }senao se(letra=="K"){
      matriz = matrizK
    }senao se(letra=="L"){
      matriz = matrizL
    }senao se(letra=="M"){
      matriz = matrizM
    }senao se(letra=="N"){
      matriz = matrizN
    }senao se(letra=="O"){
      matriz = matrizO
    }senao se(letra=="P"){
      matriz = matrizP
    }senao se(letra=="Q"){
      matriz = matrizQ
    }senao se(letra=="R"){
      matriz = matrizR
    }senao se(letra=="S"){
      matriz = matrizS
    }senao se(letra=="T"){
      matriz = matrizT
    }senao se(letra=="U"){
      matriz = matrizU
    }senao se(letra=="V"){
      matriz = matrizV
    }senao se(letra=="W"){
      matriz = matrizW
    }senao se(letra=="X"){
      matriz = matrizX
    }senao se(letra=="Y"){
      matriz = matrizY
    }senao se(letra=="Z"){
      matriz = matrizZ
    }

    escreva("\n")

    para(inteiro i = 0;i<7;i++){
      u.aguarde(50)
      para(inteiro j = 0;j<5;j++){
        se(matriz[i][j]==1){
          escreva("*")
        }senao{
          escreva(" ")
        }
      }
      escreva("\n")
    }
  }

  funcao inicio() {
    escreveLetra("A")
    escreveLetra("B")
    escreveLetra("C")
    escreveLetra("D")
    escreveLetra("E")
